<%- include('partials/header.ejs', {title: 'Admin Dashboard'}) %>

<body>
  <%- include('partials/navbar.ejs') %>
  <div class="container mt-4">
    <div class="row mb-3">
      <div class="col-12 col-md-6 mb-3">
        <div class="box bg-secondary-subtle py-auto h-100 p-4 text-center">
          <h1 class="mt-auto"><%= typeof completed !== 'undefined' ? completed : 24 %> / <%= typeof suggestions !== 'undefined' ? suggestions : 38 %>
          </h1>
          <p class="mb-auto">completions / suggestions</p>
        </div>
      </div>
      <div class="col-12 col-md-6 ">
        <div class="input-box">
          <input type="text" readonly class="text-center form-control-lg form-control-plaintext mb-3" value="<%= typeof professors.length !== 'undefined' ? professors.length : 24 %> Professors">
        </div>
        <div class="input-box">
          <input type="text" readonly class="text-center form-control-lg form-control-plaintext mb-3" value="<%= typeof courses !== 'undefined' ? courses : 13 %> Courses">
        </div>
        <div class="input-box">
          <input type="text" readonly class="text-center form-control-lg form-control-plaintext mb-3" value="<%= typeof completed !== 'undefined' ? completed : 24 %> Minutes Watched">
        </div>
      </div>
    </div>
    <div class="row mb-3">
      <div class="container" style="max-width: 700px;">
        <form class="d-flex" role="search">
          <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
          <button class="btn btn-outline-success" type="submit">Search</button>
        </form>
      </div>
    </div>
    <div class="row mb-2 gx-4">
      <% professors.forEach(prof => { %>
      <div class=" col-6 col-lg-3">
        <div class="card mb-2" >
          <img src="data:image/png;base64," class="card-img-top" alt="...">
          <div class="card-body">
            <h3><%= prof.firstName %> <%= prof.lastName %></h3>
            <p class="card-text">
              <a class="text-decoration-none" href="mailto:<%= prof.username %>"><span class="text-dark">Email: </span><strong><%= prof.username %></strong></a>
              <br>
              <em>Last updated: </em><%= prof.updatedAt.toLocaleDateString('en-us', {year: 'numeric', month: 'short', day: 'numeric'}) %>
            </p>
          </div>
        </div>
      </div>
      <% }) %>
      <div class="mb-2 col-6 col-lg-3">
        <div class="card" >
          <div class="card-body">
            <h5 class="card-title">Add Professor</h5>
            <p class="card-text">Add professor under your management.</p>
            <a href="/admins/professors/new" class="btn btn-primary">Add now</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>

